                <ca:Milestone eventIds="Truck_l" id="TakeBreakUK_m1" name="TakeBreakUK Milestone 1">
                  <ca:Condition expression="(({infoModel./infoModel/Truck/status} == [HwGBMoving]) or ({infoModel./infoModel/Truck/status} == [HwMoving])) and this.isEventOccurring(Truck_l)" id="TakeBreakUK_m1_c" language="JEXL" name="TakeBreakUK Milestone 1 Condition" />
                </ca:Milestone>
                <ca:ProcessFlowGuard id="TakeBreakUK_pfg" name="Take break in UK Process Flow Guard" expression="not (this.isMilestoneAchieved(TakeBreakUK_m1))" />
              </ca:SubStage>
              <ca:ProcessFlowGuard id="SequenceFlow_13_pfg" expression="this.isMilestoneAchieved(SequenceFlow_3_m1) and (not (({infoModel./infoModel/Truck/status} == [ChrStill]) or ({infoModel./infoModel/Truck/status} == [HwMoving]))) and not this.isMilestoneAchieved(SequenceFlow_13_m1)" />
            </ca:SubStage>
            <ca:SubStage id="SequenceFlow_3">
              <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [LhrMoving]) or ({infoModel./infoModel/Truck/status} == [HwGBMoving])) and this.isEventOccurring(Truck_e)" id="SequenceFlow_3_dfg1" language="JEXL" name="SequenceFlow_3 Data Flow Guard 1" />
              <ca:Milestone eventIds="" id="SequenceFlow_3_m1" name="SequenceFlow_3 Milestone 1">
                <ca:Condition expression="(this.isMilestoneAchieved(TravelUK_m1))" id="SequenceFlow_3_m1_c" language="JEXL" name="SequenceFlow_3 Milestone 1 Condition" />
