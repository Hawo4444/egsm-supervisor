                <ca:Milestone eventIds="Truck_l" id="TravelUK_m1" name="TravelUK Milestone 1">
                  <ca:Condition expression="(({infoModel./infoModel/Truck/status} == [HwGBStill]) or ({infoModel./infoModel/Truck/status} == [ChrStill])) and this.isEventOccurring(Truck_l)" id="TravelUK_m1_c" language="JEXL" name="TravelUK Milestone 1 Condition" />
                </ca:Milestone>
                <ca:ProcessFlowGuard id="TravelUK_pfg" name="Travel in UK Process Flow Guard" expression="not (this.isMilestoneAchieved(TravelUK_m1))" />
              </ca:SubStage>
              <ca:ProcessFlowGuard id="SequenceFlow_3_pfg" expression="not this.isMilestoneAchieved(SequenceFlow_3_m1)" />
            </ca:SubStage>
          </ca:SubStage>
          <ca:ProcessFlowGuard id="ExclusiveGateway_1_pfg" name="ExclusiveGateway_1 Process Flow Guard" expression="not this.isMilestoneAchieved(ExclusiveGateway_1_m1) and (this.isMilestoneAchieved(LoadContainer_m1))" />
        </ca:SubStage>
        <ca:SubStage id="ExclusiveGateway_7" name="ExclusiveGateway_7 SequenceFlow_6">
          <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HwMoving])) and this.isEventOccurring(Truck_e)" id="ExclusiveGateway_7_dfg1" language="JEXL" name="ExclusiveGateway_7 Data Flow Guard 1" />
          <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HwStill])) and this.isEventOccurring(Truck_e)" id="ExclusiveGateway_7_dfg2" language="JEXL" name="ExclusiveGateway_7 Data Flow Guard 2" />
          <ca:Milestone eventIds="" id="ExclusiveGateway_7_m1" name="ExclusiveGateway_7 Milestone 1">
            <ca:Condition expression="this.isMilestoneAchieved(ExclusiveGateway_7_iteration_m2)" id="ExclusiveGateway_7_m1_c" language="JEXL" name="ExclusiveGateway_7 Milestone 1 Condition" />
          </ca:Milestone>
          <ca:SubStage id="ExclusiveGateway_7_iteration" name="ExclusiveGateway_7 SequenceFlow_6">
            <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HwMoving])) and this.isEventOccurring(Truck_e)" id="ExclusiveGateway_7_iteration_dfg1" language="JEXL" name="ExclusiveGateway_7_iteration Data Flow Guard 1" />
            <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HwStill])) and this.isEventOccurring(Truck_e)" id="ExclusiveGateway_7_iteration_dfg2" language="JEXL" name="ExclusiveGateway_7_iteration Data Flow Guard 2" />
            <ca:Milestone eventIds="" id="ExclusiveGateway_7_iteration_m1" name="ExclusiveGateway_7_iteration Milestone 1">
              <ca:Condition expression="this.isMilestoneAchieved(SequenceFlow_10_m1) and ((this.isMilestoneAchieved(SequenceFlow_6_m1) and (not ({infoModel./infoModel/Truck/status} == [AmsStill])))) and not (this.isStageActive(SequenceFlow_6))" id="ExclusiveGateway_7_iteration_m1_c" language="JEXL" name="ExclusiveGateway_7_iteration Milestone 1 Condition" />
            </ca:Milestone>
            <ca:Milestone eventIds="" id="ExclusiveGateway_7_iteration_m2" name="ExclusiveGateway_7_iteration Milestone 2">
              <ca:Condition expression="this.isMilestoneAchieved(SequenceFlow_10_m1)  and ({infoModel./infoModel/Truck/status} == [AmsStill])  and not (this.isStageActive(SequenceFlow_6))" id="ExclusiveGateway_7_iteration_m2_c" language="JEXL" name="ExclusiveGateway_7_iteration Milestone 2 Condition" />
            </ca:Milestone>
            <ca:SubStage id="SequenceFlow_10">
              <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HwMoving])) and this.isEventOccurring(Truck_e)" id="SequenceFlow_10_dfg1" language="JEXL" name="SequenceFlow_10 Data Flow Guard 1" />
              <ca:Milestone eventIds="" id="SequenceFlow_10_m1" name="SequenceFlow_10 Milestone 1">
                <ca:Condition expression="(this.isMilestoneAchieved(TravelEU_m1))" id="SequenceFlow_10_m1_c" language="JEXL" name="SequenceFlow_10 Milestone 1 Condition" />
              </ca:Milestone>
              <ca:SubStage id="TravelEU" name="Travel in EU">
                <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HwMoving])) and this.isEventOccurring(Truck_e)" id="TravelEU_dfg1" language="JEXL" name="TravelEU Data Flow Guard 1" />
                <ca:Milestone eventIds="Truck_l" id="TravelEU_m1" name="TravelEU Milestone 1">
                  <ca:Condition expression="(({infoModel./infoModel/Truck/status} == [AmsStill]) or ({infoModel./infoModel/Truck/status} == [HwStill])) and this.isEventOccurring(Truck_l)" id="TravelEU_m1_c" language="JEXL" name="TravelEU Milestone 1 Condition" />
                </ca:Milestone>
                <ca:ProcessFlowGuard id="TravelEU_pfg" name="Travel in EU Process Flow Guard" expression="not (this.isMilestoneAchieved(TravelEU_m1))" />
              </ca:SubStage>
              <ca:ProcessFlowGuard id="SequenceFlow_10_pfg" expression="not this.isMilestoneAchieved(SequenceFlow_10_m1)" />
            </ca:SubStage>
            <ca:SubStage id="SequenceFlow_6">
              <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HwStill])) and this.isEventOccurring(Truck_e)" id="SequenceFlow_6_dfg1" language="JEXL" name="SequenceFlow_6 Data Flow Guard 1" />
              <ca:Milestone eventIds="" id="SequenceFlow_6_m1" name="SequenceFlow_6 Milestone 1">
                <ca:Condition expression="(this.isMilestoneAchieved(TakeBreakEU_m1))" id="SequenceFlow_6_m1_c" language="JEXL" name="SequenceFlow_6 Milestone 1 Condition" />
              </ca:Milestone>
              <ca:SubStage id="TakeBreakEU" name="Take break in EU">
                <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HwStill])) and this.isEventOccurring(Truck_e)" id="TakeBreakEU_dfg1" language="JEXL" name="TakeBreakEU Data Flow Guard 1" />
                <ca:Milestone eventIds="Truck_l" id="TakeBreakEU_m1" name="TakeBreakEU Milestone 1">
                  <ca:Condition expression="(({infoModel./infoModel/Truck/status} == [HwMoving])) and this.isEventOccurring(Truck_l)" id="TakeBreakEU_m1_c" language="JEXL" name="TakeBreakEU Milestone 1 Condition" />
                </ca:Milestone>
                <ca:ProcessFlowGuard id="TakeBreakEU_pfg" name="Take break in EU Process Flow Guard" expression="not (this.isMilestoneAchieved(TakeBreakEU_m1))" />
              </ca:SubStage>
              <ca:ProcessFlowGuard id="SequenceFlow_6_pfg" expression="this.isMilestoneAchieved(SequenceFlow_10_m1) and (not ({infoModel./infoModel/Truck/status} == [AmsStill])) and not this.isMilestoneAchieved(SequenceFlow_6_m1)" />
            </ca:SubStage>
          </ca:SubStage>
          <ca:ProcessFlowGuard id="ExclusiveGateway_7_pfg" name="ExclusiveGateway_7 Process Flow Guard" expression="not this.isMilestoneAchieved(ExclusiveGateway_7_m1) and (this.isMilestoneAchieved(TakeChannelTunnel_m1))" />
        </ca:SubStage>
        <ca:SubStage id="TakeChannelTunnel" name="Take Channel tunnel">
          <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [ChrStill])) and this.isEventOccurring(Truck_e)" id="TakeChannelTunnel_dfg1" language="JEXL" name="TakeChannelTunnel Data Flow Guard 1" />
          <ca:Milestone eventIds="Truck_l" id="TakeChannelTunnel_m1" name="TakeChannelTunnel Milestone 1">
            <ca:Condition expression="(({infoModel./infoModel/Truck/status} == [CoqStill]) or ({infoModel./infoModel/Truck/status} == [CoqMoving])) and this.isEventOccurring(Truck_l)" id="TakeChannelTunnel_m1_c" language="JEXL" name="TakeChannelTunnel Milestone 1 Condition" />
