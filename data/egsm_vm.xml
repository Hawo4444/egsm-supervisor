  <ca:Component id="Definitions_1">
    <ca:InformationModel id="infoModel" rootDataItemId="infoModel">
      <ca:DataItem id="infoModel" rootElement="infoModel" schemaUri="data/infoModel.xsd" />
    </ca:InformationModel>
    <ca:GuardedStageModel id="example_GSM" name="Default Process">
      <ca:Stage id="example" name="Default Process">
        <ca:DataFlowGuard eventIds="process_started" expression="this.isEventOccurring(process_started)" id="example_dfg1" language="JEXL" name="example Data Flow Guard 1" />
        <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [LhrStill])) and this.isEventOccurring(Truck_e)" id="example_dfg2" language="JEXL" name="example Data Flow Guard 2" />
        <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [LhrMoving]) or ({infoModel./infoModel/Truck/status} == [HwGBMoving])) and this.isEventOccurring(Truck_e)" id="example_dfg3" language="JEXL" name="example Data Flow Guard 3" />
        <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HwGBStill])) and this.isEventOccurring(Truck_e)" id="example_dfg4" language="JEXL" name="example Data Flow Guard 4" />
        <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [ChrStill])) and this.isEventOccurring(Truck_e)" id="example_dfg5" language="JEXL" name="example Data Flow Guard 5" />
        <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HwMoving])) and this.isEventOccurring(Truck_e)" id="example_dfg6" language="JEXL" name="example Data Flow Guard 6" />
        <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HwStill])) and this.isEventOccurring(Truck_e)" id="example_dfg7" language="JEXL" name="example Data Flow Guard 7" />
        <ca:DataFlowGuard eventIds="process_ended" expression="this.isEventOccurring(process_ended)" id="example_dfg8" language="JEXL" name="example Data Flow Guard 8" />
        <ca:Milestone eventIds="" id="example_m1" name="example Milestone 1">
          <ca:Condition expression="this.isMilestoneAchieved(ExclusiveGateway_1_m1) and this.isMilestoneAchieved(ExclusiveGateway_7_m1) and (this.isMilestoneAchieved(TakeChannelTunnel_m1)) and (this.isMilestoneAchieved(LoadContainer_m1)) and this.isMilestoneAchieved(process_started_m1) and this.isMilestoneAchieved(process_ended_m1)" id="example_m1_c" language="JEXL" name="example Milestone 1 Condition" />
        </ca:Milestone>
        <ca:SubStage id="ExclusiveGateway_1" name="ExclusiveGateway_1 SequenceFlow_13">
          <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [LhrMoving]) or ({infoModel./infoModel/Truck/status} == [HwGBMoving])) and this.isEventOccurring(Truck_e)" id="ExclusiveGateway_1_dfg1" language="JEXL" name="ExclusiveGateway_1 Data Flow Guard 1" />
          <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HwGBStill])) and this.isEventOccurring(Truck_e)" id="ExclusiveGateway_1_dfg2" language="JEXL" name="ExclusiveGateway_1 Data Flow Guard 2" />
          <ca:Milestone eventIds="" id="ExclusiveGateway_1_m1" name="ExclusiveGateway_1 Milestone 1">
            <ca:Condition expression="this.isMilestoneAchieved(ExclusiveGateway_1_iteration_m2)" id="ExclusiveGateway_1_m1_c" language="JEXL" name="ExclusiveGateway_1 Milestone 1 Condition" />
          </ca:Milestone>
          <ca:SubStage id="ExclusiveGateway_1_iteration" name="ExclusiveGateway_1 SequenceFlow_13">
            <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [LhrMoving]) or ({infoModel./infoModel/Truck/status} == [HwGBMoving])) and this.isEventOccurring(Truck_e)" id="ExclusiveGateway_1_iteration_dfg1" language="JEXL" name="ExclusiveGateway_1_iteration Data Flow Guard 1" />
            <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HwGBStill])) and this.isEventOccurring(Truck_e)" id="ExclusiveGateway_1_iteration_dfg2" language="JEXL" name="ExclusiveGateway_1_iteration Data Flow Guard 2" />
            <ca:Milestone eventIds="" id="ExclusiveGateway_1_iteration_m1" name="ExclusiveGateway_1_iteration Milestone 1">
              <ca:Condition expression="this.isMilestoneAchieved(SequenceFlow_3_m1) and ((this.isMilestoneAchieved(SequenceFlow_13_m1) and (not (({infoModel./infoModel/Truck/status} == [ChrStill]) or ({infoModel./infoModel/Truck/status} == [HwMoving]))))) and not (this.isStageActive(SequenceFlow_13))" id="ExclusiveGateway_1_iteration_m1_c" language="JEXL" name="ExclusiveGateway_1_iteration Milestone 1 Condition" />
            </ca:Milestone>
            <ca:Milestone eventIds="" id="ExclusiveGateway_1_iteration_m2" name="ExclusiveGateway_1_iteration Milestone 2">
              <ca:Condition expression="this.isMilestoneAchieved(SequenceFlow_3_m1)  and (({infoModel./infoModel/Truck/status} == [ChrStill]) or ({infoModel./infoModel/Truck/status} == [HwMoving]))  and not (this.isStageActive(SequenceFlow_13))" id="ExclusiveGateway_1_iteration_m2_c" language="JEXL" name="ExclusiveGateway_1_iteration Milestone 2 Condition" />
            </ca:Milestone>
            <ca:SubStage id="SequenceFlow_13">
              <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HwGBStill])) and this.isEventOccurring(Truck_e)" id="SequenceFlow_13_dfg1" language="JEXL" name="SequenceFlow_13 Data Flow Guard 1" />
              <ca:Milestone eventIds="" id="SequenceFlow_13_m1" name="SequenceFlow_13 Milestone 1">
                <ca:Condition expression="(this.isMilestoneAchieved(TakeBreakUK_m1))" id="SequenceFlow_13_m1_c" language="JEXL" name="SequenceFlow_13 Milestone 1 Condition" />
              </ca:Milestone>
              <ca:SubStage id="TakeBreakUK" name="Take break in UK">
