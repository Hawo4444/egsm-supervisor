        </ca:SubStage>
        <ca:SubStage id="LoadContainer" name="Load container">
          <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [LhrStill])) and this.isEventOccurring(Truck_e)" id="LoadContainer_dfg1" language="JEXL" name="LoadContainer Data Flow Guard 1" />
