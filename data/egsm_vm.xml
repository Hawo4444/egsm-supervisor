          <ca:Milestone eventIds="process_started" id="process_started_m1" name="process_started Milestone 1">
            <ca:Condition expression="this.isEventOccurring(process_started)" id="process_started_m1_c" language="JEXL" name="process_started Milestone 1 Condition" />
          </ca:Milestone>
          <ca:ProcessFlowGuard id="process_started_pfg" name="process started Process Flow Guard" expression="not this.isMilestoneAchieved(process_started_m1) " />
        </ca:SubStage>
        <ca:SubStage id="process_ended" name="process ended">
          <ca:DataFlowGuard eventIds="process_ended" expression="this.isEventOccurring(process_ended)" id="process_ended_dfg1" language="JEXL" name="process_ended Data Flow Guard 1" />
          <ca:Milestone eventIds="process_ended" id="process_ended_m1" name="process_ended Milestone 1">
            <ca:Condition expression="this.isEventOccurring(process_ended)" id="process_ended_m1_c" language="JEXL" name="process_ended Milestone 1 Condition" />
          </ca:Milestone>
          <ca:ProcessFlowGuard id="process_ended_pfg" name="process ended Process Flow Guard" expression="not this.isMilestoneAchieved(process_ended_m1)  and this.isMilestoneAchieved(ExclusiveGateway_7_m1)" />
        </ca:SubStage>
      </ca:Stage>
    </ca:GuardedStageModel>
  </ca:Component>
</ca:CompositeApplicationType>