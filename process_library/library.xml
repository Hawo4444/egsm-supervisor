<?xml version="1.0" encoding="UTF-8"?>
<library>
    <!-- Inland Transportation Process-->
    <process-type>
        <name>Inland_transportation_process</name>
        <bpmn-model>
            <model>./bpmn_models/inland_transportation.bpmn</model>
        </bpmn-model>

        <perspective>
            <name>Truck</name>
            <egsm-model>./egsm_models/inland_transportation_truck.xml</egsm-model>
            <info-model>./info_models/inland_transportation_truck.xsd</info-model>
            <bindings>./bindings/inland_transportation_truck.xml</bindings>
        </perspective>

        <perspective>
            <name>Container</name>
            <egsm-model>./egsm_models/inland_transportation_container.xml</egsm-model>
            <info-model>./info_models/inland_transportation_container.xsd</info-model>
            <bindings>./bindings/inland_transportation_container.xml</bindings>
        </perspective>
    </process-type>

    <!-- Combined Inland Transportation Process-->
    <process-type>
        <name>Combined_inland_transportation_process</name>
        <bpmn-model>
            <model>./bpmn_models/combined_inland_transportation.bpmn</model>
        </bpmn-model>

        <perspective>
            <name>Truck</name>
            <egsm-model>./egsm_models/combined_inland_transportation_truck.xml</egsm-model>
            <info-model>./info_models/combined_inland_transportation_truck.xsd</info-model>
            <bindings>./bindings/combined_inland_transportation_truck.xml</bindings>
        </perspective>

        <perspective>
            <name>Container</name>
            <egsm-model>./egsm_models/combined_inland_transportation_container.xml</egsm-model>
            <info-model>./info_models/combined_inland_transportation_container.xsd</info-model>
            <bindings>./bindings/combined_inland_transportation_container.xml</bindings>
        </perspective>

        <perspective>
            <name>Train</name>
            <egsm-model>./egsm_models/combined_inland_transportation_train.xml</egsm-model>
            <info-model>./info_models/combined_inland_transportation_train.xsd</info-model>
            <bindings>./bindings/combined_inland_transportation_train.xml</bindings>
        </perspective>
    </process-type>
</library>