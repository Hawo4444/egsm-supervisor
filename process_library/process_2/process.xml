<?xml version="1.0" encoding="UTF-8"?>
<process-type>
    <name>Inland Transportation Process Complex</name>
    <description>The process describes the inland transportation of a container by truck and train</description>

    <bpmn-model>
        <model>models/process.bpmn</model>
    </bpmn-model>

    <perspective>
        <name>Truck</name>
        <egsm-model>models/egsm_truck.xml</egsm-model>
        <info-model>models/info_truck.xsd</info-model>
        <bindings>models/binding_truck.xml</bindings>
    </perspective>

    <perspective>
        <name>Train</name>
        <egsm-model>models/egsm_train.xml</egsm-model>
        <info-model>models/info_train.xsd</info-model>
        <bindings>models/binding_train.xml</bindings>
    </perspective>

    <perspective>
        <name>Container</name>
        <egsm-model>models/egsm_container.xml</egsm-model>
        <info-model>models/info_container.xsd</info-model>
        <bindings>models/binding_container.xml</bindings>
    </perspective>
</process-type>